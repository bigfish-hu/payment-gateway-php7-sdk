{
	"name": "bigfish/paymentgateway-php7-sdk",
	"type": "library",
	"description": "BIG FISH Payment Gateway - PHP7 SDK",
	"keywords": [
		"payment",
		"ecommerce",
		"webshop"
	],
	"homepage": "https://www.paymentgateway.hu/",
	"license": "MIT",
	"authors": [
		{
			"name": "BIG FISH",
			"email": "cafe@bigfish.hu"
		}
	],
	"require": {
		"php": ">=7.2",
		"ext-curl": "*",
		"ext-json": "*",
		"ext-openssl": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "~5"
	},
	"autoload": {
		"psr-4": {
			"BigFish\\": "src/BigFish"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"BigFish\\Tests\\": "tests/BigFish"
		}
	},
	"minimum-stability": "dev",
	"scripts": {
		"phpunit": "phpunit --color=always ./tests/"
	},
	"support": {
		"email": "paymentgateway@bigfish.hu"
	}
}
